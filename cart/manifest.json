{
    "name": "Save My Tabs",
    "description": "Save your tabs for later.",
    "version": "0.0.0.5",
    "manifest_version": 3,
    "action": {
        "default_popup": "/popup/popup.html",
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    },
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "permissions": ["storage", "tabs"]
}